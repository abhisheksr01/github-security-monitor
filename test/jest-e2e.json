{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "reporters": [
      "default",
      [
        "jest-html-reporters",
        {
          "publicPath": "./reports",
          "filename": "jest-e2etest-report.html",
          "expand": true
        }
      ]
    ]
}
